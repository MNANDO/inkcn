{
    "$schema": "https://ui.shadcn.com/schema/registry.json",
    "name": "inkcn",
    "homepage": "https://inkcn.vercel.app",
    "items": [
        {
            "name": "editor",
            "type": "registry:block",
            "title": "Editor",
            "description": "A Notion style rich text editor created with Lexical.",
            "dependencies": [
                "lexical",
                "@lexical/react",
                "@lexical/extension"
            ],
            "registryDependencies": [
                "toggle-group",
                "button",
                "dropdown-menu"
            ],
            "files": [
                {
                    "path": "registry/blocks/editor-view.tsx",
                    "type": "registry:component"
                },
                {
                    "path": "registry/components/editor-toolbar.tsx",
                    "type": "registry:component"
                },
                {
                    "path": "registry/components/editor-toolbar-plugin.tsx",
                    "type": "registry:component"
                },
                {
                    "path": "registry/components/editor-block-control-plugin.tsx",
                    "type": "registry:component"
                },
                {
                    "path": "registry/hooks/use-editor-toolbar.ts",
                    "type": "registry:hook"
                },
                {
                    "path": "registry/hooks/use-create-editor.ts",
                    "type": "registry:hook"
                },
                {
                    "path": "registry/lib/editor-utils.ts",
                    "type": "registry:lib"
                },
                {
                    "path": "registry/lib/Editor.ts",
                    "type": "registry:lib"
                },
                {
                    "path": "registry/lib/base-nodes.ts",
                    "type": "registry:lib"
                },
                {
                    "path": "registry/lib/default-theme.ts",
